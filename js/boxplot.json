{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "path/to/filtered_top10_data.csv"
    },
    "mark": {
      "type": "boxplot",
      "extent": "min-max"
    },
    "encoding": {
      "x": {
        "field": "COUNTRY",
        "type": "nominal",
        "title": "Country"
      },
      "y": {
        "field": "RUNUP_HT",
        "type": "quantitative",
        "title": "Run-up Height (feet)"
      },
      "color": {
        "field": "COUNTRY",
        "type": "nominal",
        "legend": {"title": "Country"}
      }
    },
    "config": {
      "axis": {"labelFontSize": 12, "titleFontSize": 14}
    }
  }
  