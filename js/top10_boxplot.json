{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "data": {
      "url": "https://raw.githubusercontent.com/Szeyuinn/FIT3179Assignment/refs/heads/main/data/runup_top10.csv"
    },
    "description": "Run-up Heights by Top 10 Countries",
    "mark": {
      "type": "boxplot",
      "extent": "min-max"
    },
    "encoding": {
      "x": {
        "field": "COUNTRY",
        "type": "nominal",
        "title": "Country",
        "scale": {"padding": 0.3} 
      },
      "y": {
        "field": "RUNUP_HT",
        "type": "quantitative",
        "title": "Run-up Height (feet)"
      },
      "color": {
        "field": "COUNTRY",
        "type": "nominal",
        "legend": null,
        "scale":{"scheme":"tableau20"}
      }
    },
    "config": {
      "axis": {"labelFontSize": 10, "titleFontSize": 12}
    }
  }
  